{
  "name": "archie",
  "productName": "archie",
  "version": "1.0.0",
  "description": "Archie - software architecture tool",
  "main": "./.webpack/main",
  "scripts": {
    "dev:desktop": "electron-forge start",
    "dev:web": "webpack-dev-server --config webpack.web.config.js",
    "package": "electron-forge package",
    "build": "electron-forge make",
    "publish": "electron-forge publish",
    ".deploy": "run(){ aws s3 cp ./out/make/deb/x64/archie*.deb s3://$AWS_S3_BUCKET_PATH/archie.deb; }; run",
    "deploy": "run(){ dotenv -e .env.$1 -- yarn .deploy; }; run",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "jest",
    "postinstall": "patch-package"
  },
  "keywords": [],
  "author": {
    "name": "Stephen A",
    "email": "stephen@creativelogic.me"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "archie"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./public/index.html",
                  "js": "./src/client/index.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@electron-forge/cli": "6.0.0-beta.47",
    "@electron-forge/maker-deb": "6.0.0-beta.47",
    "@electron-forge/maker-rpm": "6.0.0-beta.47",
    "@electron-forge/maker-squirrel": "6.0.0-beta.47",
    "@electron-forge/maker-zip": "6.0.0-beta.47",
    "@electron-forge/plugin-webpack": "6.0.0-beta.47",
    "@hot-loader/react-dom": "^16.11.0",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/jest": "^25.1.1",
    "@types/js-yaml": "^3.12.2",
    "@types/node": "^13.7.4",
    "@types/ramda": "^0.26.40",
    "@types/reach__router": "^1.2.6",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "dotenv-cli": "^3.1.0",
    "electron": "7.1.11",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^25.1.0",
    "node-loader": "^0.6.0",
    "node-sass": "^4.13.1",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "raw-loader": "^4.0.0",
    "react-hot-loader": "^4.12.19",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.0",
    "typescript": "^3.7.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.6",
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@projectstorm/react-canvas-core": "^6.0.2",
    "@projectstorm/react-diagrams": "^6.0.2",
    "@reach/router": "^1.3.0",
    "antd": "^4.1.1",
    "change-case": "^4.1.1",
    "closest": "^0.0.1",
    "core-js": "^3.6.4",
    "dagre": "^0.8.5",
    "electron-context-menu": "^0.16.0",
    "electron-squirrel-startup": "^1.0.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "mathjs": "^6.6.0",
    "pathfinding": "^0.4.18",
    "paths-js": "^0.4.10",
    "ramda": "^0.27.0",
    "react": "^16.12.0",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-pie-menu": "^0.2.5",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.1",
    "regenerator-runtime": "^0.13.3",
    "resize-observer-polyfill": "^1.5.1",
    "spec-codegen": "git+ssh://git@gitlab.com:continuouss/arch/spec-codegen",
    "styled-components": "^5.0.1"
  }
}
