{
  "name": "node-editor",
  "productName": "node-editor",
  "version": "0.1.0",
  "description": "Node editor",
  "main": "./.webpack/main",
  "scripts": {
    "dev:web": "webpack-dev-server --config webpack.web.config.js",
    "dev:nodegui-server": "webpack --mode=development --config webpack.nodegui.config.js",
    "dev:nodegui": "qode ./dist-nodegui/index.js",
    "debug:nodegui": "qode --inspect ./dist-nodegui/index.js",
    "build:nodegui": "webpack -p --config webpack.nodegui.config.js",
    "build:node-rust": "bash -c '[[ \"$(qode --version)\" == \"$(node --version)\" ]] && neon build -p rust --release || echo \"node version must be same as qode ($(qode --version))\"'",
    "build:wasm-rust": "wasm-pack build ./rust/wasm -- -p wasm",
    "build:rust": "cargo build -p native --manifest-path ./rust/Cargo.toml --release",
    "clean:node-rust": "neon clean -p rust",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "jest",
    "postinstall": "patch-package"
  },
  "keywords": [],
  "author": {
    "name": "Stephen A",
    "email": "stephen@creativelogic.me"
  },
  "license": "MIT",
  "engines": {
    "node": "13.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@hot-loader/react-dom": "^16.11.0",
    "@nodegui/packer": "^1.4.0",
    "@types/jest": "^25.1.1",
    "@types/node": "^13.7.4",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^25.1.0",
    "native-addon-loader": "^2.0.1",
    "neon-cli": "^0.4.0",
    "node-loader": "^0.6.0",
    "node-sass": "^7.0.0",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "react-hot-loader": "^4.12.19",
    "sass-loader": "^8.0.2",
    "shadow-cljs": "^2.10.1",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.0",
    "typescript": "^3.7.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@nodegui/nodegui": "^0.20.0",
    "@nodegui/qode": "^2.1.0",
    "@nodegui/react-nodegui": "^0.6.1",
    "open": "^7.0.3",
    "react": "^16.12.0",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-scripts": "^3.3.1",
    "source-map-support": "^0.5.19"
  }
}
